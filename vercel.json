{
    "version": 2,
    "builds": [
      {
        "src": "cs412/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
      },
      {
        "src": "static/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [

      {
        "src": "/(.*)",
        "dest": "cs412/wsgi.py"
      }
    ]
  }
  