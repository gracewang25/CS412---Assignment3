{
    "version": 2,
    "builds": [
      {
        "src": "cs412/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
      },
      {
        "src": "static/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "cs412/wsgi.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "cs412.settings",
      "SECRET_KEY": "@your-secret-key",
      "DEBUG": "False",
      "DATABASE_URL": "@your-database-url"
    }
  }
  